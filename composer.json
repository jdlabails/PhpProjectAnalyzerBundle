{
  "name": "jdlabails/php-project-analyzer-bundle",
  "description": "Symfony Bundle for PhpProjectAnalyzer",
  "type": "symfony-bundle",
  "homepage": "https://github.com/jdlabails/PhpProjectAnalyzerBundle",
  "license": [
    "MIT"
  ],
  "keywords": [
    "Php Project Analyzer Symfony"
  ],
  "authors": [
    {
      "name": "Jean-David Labails",
      "email": "jd.labails@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/symfony": ">=2.3",
    "symfony/translation": ">=2.3",
    "sebastian/phpcpd": "*",
    "phploc/phploc": "*",
    "symfony/phpunit-bridge": "*",
    "squizlabs/php_codesniffer": "*",
    "pdepend/pdepend": "*",
    "phpdocumentor/phpdocumentor": "*",
    "phpmd/phpmd" : "*"
  },
  "scripts": {
    "post-install-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
    ],
    "post-update-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
    ]
  },
  "autoload": {
    "psr-4": {
      "JD\\PhpProjectAnalyzerBundle\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
