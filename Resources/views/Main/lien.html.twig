<div class="panel panel-info">
    <div class="panel-heading">
        <h3>            
            <span class="glyphicon glyphicon-link"></span>
            {% trans %}link.link{%endtrans%}
        </h3>
        <div style=" font-size: 75%;color: #777;">
            {% trans %}link.tools{%endtrans%}
        </div>
    </div>
    <div class="panel-body" style="line-height: 30px">
        <a href="{{ app.request.getSchemeAndHttpHost()}}/ppa/reports/TEST/phpUnitReport/dashboard.html" target="_blank">
            {% trans %}link.coverageReport{%endtrans%}
        </a>
        <br>

        {% if projectAnalyzer.isEnable('docs') %}
        <a href="{{ app.request.getSchemeAndHttpHost()}}/ppa/reports/DOCS/index.html" target="_blank">
            {% trans %}link.phpDoc{%endtrans%}
        </a>
        {% endif %}

        {% if projectAnalyzer.isEnable('histo', true) %}
        <br>
        <a href="histo.php" target="_blank">
            {% trans %}link.histo{%endtrans%}
        </a>
        {% endif %}

        <br>
        <a href="{{path ('jd_php_project_analyzer_phpinfo')}}" target="_blank">
            {% trans %}link.phpinfo{%endtrans%}
        </a>
        <hr>
        <a href="{{ params['gitRepositoryURL']}}" target="_blank">
            {% trans %}link.gitRepositoryAccess{%endtrans%}
        </a>
        <br>
    </div>
</div>
