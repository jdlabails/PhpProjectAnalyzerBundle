{% for idAnalyse, title in projectAnalyzer.tabAvailableAnalysis %}
    {% if projectAnalyzer.isEnable(idAnalyse, true) %}        
        {% if idAnalyse == 'test' %}
            {% set report = projectAnalyzer.testInfo %}
        {% else %}
            {% set report = projectAnalyzer.reportInfo[idAnalyse|upper] %}
        {% endif %}
        
        {% include 'JDPhpProjectAnalyzerBundle:Main:blockReportDetail.html.twig' %}
        
        {% include 'JDPhpProjectAnalyzerBundle:Main:Modal/rapport.html.twig' %}
        {% include 'JDPhpProjectAnalyzerBundle:Main:Modal/cmd.html.twig' %}
        {% include 'JDPhpProjectAnalyzerBundle:Main:Modal/cmdManuelle.html.twig' %}
        
        {% if idAnalyse == 'cs' %}
            {% include 'JDPhpProjectAnalyzerBundle:Main:Modal/cmdRep.html.twig' %}
        {% endif %}
    {% endif %}
{% endfor %}
