{
    "name" : "jdlabails/php-project-analyzer-bundle",
    "description" : "Bundle for PhpProjectAnalyzer",
    "type" : "symfony-bundle",    
    "homepage": "https://github.com/jdlabails/PhpProjectAnalyzerBundle",
    "license" : ["MIT"],
    "keywords" : ["Php Project Analyzer"],
    "authors" : [{
        "name" : "Jean-David Labails",
        "email" : "jd.labails@gmail.com"
    }],
    "require" : {        
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.7",
        "symfony/twig-bundle": "~2.3",
        "ext-xdebug": ">=2.1.4"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd": [
            "JD\\PhpProjectAnalyzerBundle\\Resources\\_phar\\update.sh",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "autoload" : {
        "psr-4": { "JD\\PhpProjectAnalyzerBundle\\": "" }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "0.x-dev"
        }
    }
}